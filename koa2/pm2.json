{
  "apps": [
    {
      "name": "reptile",
      "script": "./bin/www",
      "cwd": "./",
      "watch": [
        "*"
      ],
      "ignore_watch": [
        "logs",
        "node_modules",
        "upload"
      ],
      "instances": 1,
      "error_file": "./logs/err.log",
      "out_file": "./logs/out.log",
      "log_date_format": "YYYY-MM-DD HH:mm Z"
    }
  ]
}